{
  "name": "action-bump-version",
  "version": "0.0.1",
  "private": true,
  "description": "Automatically bump contents of version file",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snap40/action-bump-version.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Current Health",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@octokit/rest": "^17.0.0",
    "@octokit/webhooks": "^7.1.0",
    "@types/semver": "^7.1.0",
    "semver": "^7.1.3",
    "simple-git": "^1.131.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
